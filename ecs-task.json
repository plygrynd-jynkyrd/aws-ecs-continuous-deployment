{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 7000,
          "protocol": "tcp",
          "containerPort": 7000
        }
      ],
      "name": "sample-cd",
      "cpu": 256,
      "memory": 512,
      "image": "leonardiwagner/cd",
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::810028704317:role/ecsTaskExecutionRole",
  "family": "cd-task"
}